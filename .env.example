# EmailJS Configuration
# Copy this file to .env.local and fill in your actual values

# EmailJS Service ID - Get this from your EmailJS dashboard
NEXT_PUBLIC_EMAILJS_SERVICE_ID=service_your_service_id

# EmailJS Template ID - Get this from your EmailJS dashboard
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=template_your_template_id

# EmailJS Public Key - Get this from your EmailJS dashboard
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key

# Contact Email (for reference)
CONTACT_EMAIL=your-email@gmail.com
